{
  "version": 2,
  "builds": [
    {
      "src": "valentinesite/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "images": {
    "sizes": [640, 750, 828, 1080, 1200],
    "dangerouslyAllowSVG": true
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "valentinesite/wsgi.py"
    }
  ]
}